command_oldcar_Impl(playerid, params[256])
{
    new string[64];

    if(IsPlayerConnected(playerid))
    {
	    format(string, sizeof(string), "Twój stary pojazd ma numer: %d",gLastCar[playerid]);
	    sendTipMessageEx(playerid, COLOR_GREY, string);
    }
    return 1;
}

