CMD:gotoadmin(playerid)
{
    if(PlayerInfo[playerid][pAdmin] < 1) return 1;
    SetPlayerVirtualWorld(playerid, 5000);
	SetPlayerPosEx(playerid, 3524.550292, -236.021621, 35.619190);
	
    Wchodzenie(playerid);
    return 1;
}

