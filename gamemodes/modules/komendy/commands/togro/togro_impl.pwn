//------------------<[ Implementacja: ]>-------------------
command_togro_Impl(playerid, params[256])
{
    if(IsPlayerConnected(playerid))
    {
		if (gRO[playerid] == 0)
		{
			gRO[playerid] = 1;
            MSGBOX_Show(playerid, "Czat RO ~r~OFF", MSGBOX_ICON_TYPE_OK);
		}
		else if (gRO[playerid] == 1)
		{
			gRO[playerid]  = 0;
            MSGBOX_Show(playerid, "Czat RO ~g~ON", MSGBOX_ICON_TYPE_OK);
		}
	}
	return 1;
}

