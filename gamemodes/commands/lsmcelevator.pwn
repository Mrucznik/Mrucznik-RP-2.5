CMD:lsmcelevator(playerid)
{
    if(PlayerInfo[playerid][pAdmin] < 20) return 1;
    ChangeLSMCElevatorState();
    sendTipMessageEx(playerid, COLOR_GREEN, "OK.");
    return 1;
}


