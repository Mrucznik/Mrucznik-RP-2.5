command_komendycmd_Impl(playerid, params[256])
{
    new count = Command_GetPlayerCommandCount(playerid);
    for (new i = 0; i != count; ++i)
    {
        SendClientMessage(playerid, 0xFF0000AA, Command_GetNext(i, playerid));
    }
    return 1;
}

