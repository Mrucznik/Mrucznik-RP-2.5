command_zoneid_Impl(playerid)
{
    if(GetPVarInt(playerid, "zoneid") != -1)
    {
        new str[32];
        format(str, 32, "Zone: %d", GetPVarInt(playerid, "zoneid"));
        SendClientMessage(playerid, -1, str);
    }
    return 1;
}

