command_loadconfig_Impl(playerid)
{
    if(!Uprawnienia(playerid, ACCESS_OWNER)) return 1;
    SendClientMessage(playerid, -1, "Wczytujê konfiguracjê serwera.");
    LoadConfig();
    SendClientMessage(playerid, -1, "OK");
    //
    return 1;
}

