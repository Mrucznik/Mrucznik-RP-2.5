command_setgroup_Impl(playerid, giveid, uid)
{
    if(!IsAScripter(playerid) && !Uprawnienia(playerid, ACCESS_MAKELEADER))
    {
        return 1;
    }

    if(GroupInfo[uid][UID] == 0 || uid > MAX_GROUPS)
    {
        sendTipMessage(playerid, "B³êdne UID grupy.");
        return 1;
    }

    
    return 1;
}

