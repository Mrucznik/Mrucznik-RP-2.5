CMD:auto(playerid) return cmd_car(playerid);
CMD:pojazd(playerid) return cmd_car(playerid);
CMD:samochod(playerid) return cmd_car(playerid);
CMD:car(playerid)
{
    if(IsPlayerConnected(playerid))
    {
        if(CountPlayerCars(playerid) > 0)
        {
  		    ShowCarsForPlayer(playerid, playerid);
		}
		else
		{
		    SendClientMessage(playerid, COLOR_GRAD2, "Nie posiadasz w³asnego pojazdu");
		}
	}
	return 1;
}

