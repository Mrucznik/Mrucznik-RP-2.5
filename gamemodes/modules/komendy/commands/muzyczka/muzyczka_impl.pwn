{
    new muzyczka;
	if(sscanf(params, "d", muzyczka))
	{
		SendClientMessage(playerid,COLOR_P@,"U¯YJ: /muzyczka [id muzyki]");
		return 1;
	}
    PlayerPlaySound(playerid, muzyczka, 0.0, 0.0, 0.0);
	return 1;
}

