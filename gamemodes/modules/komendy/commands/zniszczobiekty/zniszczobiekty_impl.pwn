        new string[128];
        format(string, 128, "CMD_Info: /zniszczobiekty u¿yte przez %s [%d]", GetNickEx(playerid), playerid);
        SendCommandLogMessage(string);
		Log(adminLog, INFO, "Admin %s u¿y³ /zniszczobiekty", GetPlayerLogName(playerid));
    }
	return 1;
}

